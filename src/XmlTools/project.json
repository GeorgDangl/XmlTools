{
  "version": "0.1.0",
  "authors": [ "Georg Dangl" ],
  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50",
      "dependencies": {
        "NETStandard.Library": "1.6.0",
        "System.Xml.XDocument": "4.3.0"
      }
    },
    "net46": {
      "frameworkAssemblies": {
        "System.Xml.Linq": "4.0.0.0",
        "System.Xml": "4.0.0.0"
      }
    }
  },
  "packOptions": {
    "projectUrl": "https://github.com/GeorgDangl/XmlTools",
    "repository": {
      "url": "https://github.com/GeorgDangl/XmlTools"
    },
    "licenseUrl": "https://github.com/GeorgDangl/XmlTools/blob/master/LICENCE.md",
    "iconUrl": "https://blog.dangl.me/media/1114/gd_icon_256.png",
    "owners": [ "Georg Dangl" ],
    "tags": [ "Xml Schema", "XSD", "Xml Validation" ],
    "files": {
      "mappings": {
        "Tool/netcoreapp1.0/": {
          "include": "../XmlTools.Console/bin/Release/netcoreapp1.0",
          "exclude": "../XmlTools.Console/bin/Release/netcoreapp1.0/*.pdb"
        },
        "Tool/net46/": {
          "include": "../XmlTools.Console/bin/Release/net46/win7-x64",
          "exclude": "../XmlTools.Console/bin/Release/net46/win7-x64/*.pdb"
        }
      }
    }
  }
