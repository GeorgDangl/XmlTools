	<xs:complexType name="Formel1">
		<xs:annotation>
			<xs:documentation>Summe von max. 4 Werten</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="X1" type="formelwert.type"/>
			<xs:element name="X2" type="formelwert.type" minOccurs="0"/>
